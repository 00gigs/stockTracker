<body>
{{!--THIS IS HANDLEBARS COMMENT SYNTAX--}}
<br>
{{ticker}}
<div class="card mb-3 class=p-3 mb-2 bg-secondary text-white style= width: 18rem  text-center ;" >
     <div class="card-header bg-info">Stock</div>
  <div class="card-body card-bg">
    <h5 class="card-title"> <b> {{stock.ticker}}</b>  </h5>
    <p class="card-text">   
{{!--basically stock.results.this.👇🏻--}}
{{#each stock.results}}
<strong>open</strong> {{this.o}} <br>
<strong>close</strong> {{this.c}} <br>
<strong>high</strong> {{this.h}} <br>
<strong>low</strong> {{this.l}} <br>
<strong>Volume_Weight_Avg.</strong> {{this.vw}} <br>
<strong>timeStamp</strong> {{this.t}} <br>
{{/each}}
<br>
 </p>
